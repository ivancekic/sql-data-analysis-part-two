			     , ps.BROJ_SASTAVNICE
                 , nvl(KOLICINA,0)  							KOLICINA_SKL_JM
                 , nvl(KOLICINA,0) * faktor_planske 			KOLICINA
                 , nvl(OPTIMALNA_ZALIHA,0)						OPTIMALNA_ZALIHA_SKL_JM
