			     , nvl(PLANIRANA_PRODAJA,0)						PLANIRANA_PRODAJA_SKL_JM
			     , nvl(PLANIRANA_PRODAJA,0) * faktor_planske	PLANIRANA_PRODAJA

			From planiranje_stavka ps
		       , proizvod p
