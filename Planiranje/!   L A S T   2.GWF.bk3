  and ps.broj_dok				in (&c_broj_dok);

DELETE
From PLANIRANJE_ZAGLAVLJE ps
WHERE ps.plan_tip_id			= &c_tip_id
  and ps.PLAN_CIKLUS_ID			= &c_ciklus_id
  and ps.PLAN_period_ID			= &c_period_id
  and ps.plan_trajanje_id		IN( &c_trajanje_id )
  and ps.broj_dok				in (&c_broj_dok);

COMMIT;
