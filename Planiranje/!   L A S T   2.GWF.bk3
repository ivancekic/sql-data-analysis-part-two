               			      where kv.proizvod = ps.proizvod
               			         and TIP='K'
               			     )
               	   END DOBAVLJAC

			  	 , CASE WHEN ps.broj_dok in (2,4) THEN
                             (select NABAVNA_SIFRA from katalog_view kv
               			      where kv.proizvod = ps.proizvod
               			         and TIP='T'
               			     )
               	   ELSE
                             (select NABAVNA_SIFRA from katalog_view kv
               			      where kv.proizvod = ps.proizvod
               			         and TIP='K'
               			     )
               	   END NABAVNA_SIFRA

