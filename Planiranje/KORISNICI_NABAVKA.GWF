"REM WORKSPACETAB3","UPISI U PLAN_PRO_KOR.sql",,27

INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@RUBIN
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@RUBIN WHERE TIP_PROIZVODA NOT IN (1,2) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@MONUS
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@MONUS WHERE TIP_PROIZVODA NOT IN (1,2,12) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@INVEJ
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@INVEJ GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@RATAR
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@RATAR WHERE TIP_PROIZVODA NOT IN (1,16) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@PEKARA
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@PEKARA WHERE TIP_PROIZVODA NOT IN (1,16) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@ALBUS
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@ALBUS WHERE TIP_PROIZVODA NOT IN (1,2) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@VITAL
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@VITAL WHERE TIP_PROIZVODA NOT IN ('01','02') GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@MEDELA
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@MEDELA WHERE TIP_PROIZVODA NOT IN (1,6) GROUP BY TIP_PROIZVODA
/
INSERT INTO PLANIRANJE_PROIZVOD_KORISNIK@SUNCE
SELECT 'NNEVENAM','TIP_PROIZVODA',TIP_PROIZVODA FROM PROIZVOD@SUNCE WHERE TIP_PROIZVODA NOT IN (1,11) GROUP BY TIP_PROIZVODA
