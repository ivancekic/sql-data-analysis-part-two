"REM WORKSPACETAB0",Korisnici.sql,,43
SELECT * FROM
(
select '01rub' firma, username, IME, PREZIME, OTAC from korisnik@rubin

union
select '02inv' firma, username, IME, PREZIME, OTAC from korisnik@invej
union
select '03mon' firma, username, IME, PREZIME, OTAC from korisnik@monus
union
select '04kos' firma, username, IME, PREZIME, OTAC from korisnik@kosava
union
select '041happy' firma, username, IME, PREZIME, OTAC from korisnik@happy

union
select '05rat' firma, username, IME, PREZIME, OTAC from korisnik@ratar
union
select '06pek' firma, username, IME, PREZIME, OTAC from korisnik@pekara
union
select '07luk' firma, username, IME, PREZIME, OTAC from korisnik@lukap

union
select '08alb' firma, username, IME, PREZIME, OTAC from korisnik@albus

union
select '09vit' firma, username, IME, PREZIME, OTAC from korisnik@vital
union
select '10med' firma, username, IME, PREZIME, OTAC from korisnik@medela

union
select '11sun' firma, username, IME, PREZIME, OTAC from korisnik@sunce
)
WHERE --username = 'ALEKSADAR'

      '%'||UPPER(USERNAME)||'%' LIKE( '%NEVENA%' )
  AND '%'||UPPER(IME)||'%' LIKE( '%NEVENA%' )
  AND '%'||UPPER(PREZIME)||'%' LIKE( '%MARIN%' )
-- AND UPPER(PREZIME) LIKE( 'KLIP%' )
  --AND SUBSTR(UPPER(PREZIME),1,LENGTH(PREZIME)-1) = 'RUDAKOVI'
--AND  '%'||upper(otac)||'%' like '%INV%'
--
--and username = 'NMAJAI'
ORDER BY FIRMA, USERNAME
--USERNAME IN ('NJELENAO')
